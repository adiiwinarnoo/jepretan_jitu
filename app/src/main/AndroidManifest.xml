        android:icon="@drawable/icon_jepretan"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon_jepretan"
        android:supportsRtl="true"
        android:theme="@style/Theme.JepretaJitu"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">
        <activity
            android:name=".view.activity.fotographer.KatalogFotoActivity"
            android:exported="false" />
        <activity
            android:name=".view.activity.user.UlasanActivity"
            android:exported="false" />
        <activity
            android:name=".view.activity.fotographer.KelolaPesananActivity"
            android:exported="false" />
        <activity
            android:name=".view.activity.admin.ApprovePaymentActivity"
            android:exported="false" />
        <activity
            android:name=".view.activity.user.PaymentActivity"
            android:exported="false" />
        <activity
            android:name=".view.activity.UbahProfileActivity"
            android:exported="false" />
        <activity
            android:name=".view.activity.ProfileActivity"
            android:exported="false" />
        <activity
            android:name=".view.activity.user.DetailKatalogActivity"
            android:exported="false" />
        <activity
            android:name=".view.activity.user.LihatKatalogActivity"
            android:exported="false" />
        <activity
            android:name=".view.activity.user.MUtamaActivity"
            android:exported="false" />
        <activity
            android:name=".view.activity.ForgotPasswordActivity"
            android:exported="false" />
        <activity
            android:name=".view.activity.fotographer.UploadKatalogActivity"
            android:exported="true" />
        <activity
            android:name=".view.activity.fotographer.AddFotoActivity"
            android:exported="false" />
        <activity
            android:name=".view.activity.admin.MenuUtama"
            android:exported="false" />
        <activity
            android:name=".view.activity.RegisterActivity"
            android:exported="false" />
        <activity
            android:name=".view.activity.SplashScreenActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.activity.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true" />
    </application>

</manifest>